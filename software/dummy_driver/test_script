#!/bin/sh
echo 'Measuring the GREP throughput. Waiting 10 seconds and type "wc output.txt"'
echo grep '"001111111111\|002222222222\|003333333333\|004444444444\|FFFFFFFFFFFF" < /dev/ethpipe > output.txt'
grep "001111111111\|002222222222\|003333333333\|004444444444\|FFFFFFFFFFFF" < /dev/ethpipe > output.txt & sleep 10; kill %1
